            y + 2 + (ds_list_find_value(ds_map_find_value(weapon_y, spr_player), frame_index) - sprite_get_yoffset(spr_player)),
            1, scale_x, angle, c_white, 1);
    }
    if(behind) {
        draw_sprite_ext(spr_player, animation_index, x, y + 2, scale_x, 1, 0, color, 1);
    }
}
